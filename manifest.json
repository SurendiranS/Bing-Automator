{
    "name": "Bing Automator",
    "version": "1.4.0",
    "description": "Chrome / Edge extension for Automatic Bing Search",
    "manifest_version": 3,
    "author": "Surendiran",
    "action": {
        "default_popup": "index.html",
        "default_title": "Bing Automator"
    },
    "permissions": [
        "debugger"
    ],
    "icons": {
        "16": "./img/icon16.png",
        "48": "./img/icon48.png",
        "128": "./img/icon128.png"
    },
    "background": {
        "service_worker": "./js/background.js"
    }
}
